  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint server/capacity/tables.ts server/capacity/telemetry.ts server/capacity/selector.ts tests/server/capacity/telemetry.sanitization.test.ts --ext .ts,.tsx,.js",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit -p tsconfig.strict.json",
    "validate:env": "tsx scripts/validate-env.ts",
    "env:sync-example": "tsx scripts/generate-env-local-example.ts",
    "prebuild": "npm run validate:env",
    "predev": "npm run validate:env",
    "reserve:dev": "vite --config reserve/vite.config.ts",
    "reserve:build": "vite build --config reserve/vite.config.ts",
    "test": "vitest run --config reserve/vitest.config.ts",
    "test:watch": "vitest --config reserve/vitest.config.ts",
    "test:ops": "vitest run --config vitest.config.ts",
    "test:e2e": "PLAYWRIGHT_AUTH_REFRESH=true playwright test",
    "storybook": "storybook dev -p 6006 -c reserve/.storybook",
    "storybook:build": "storybook build -c reserve/.storybook -o reserve/storybook-static",
    "analyze": "vite build --config reserve/vite.config.ts --mode analyze",
    "prepare": "husky",
    "secret:scan": "gitleaks detect --no-banner --redact && trufflehog filesystem --no-update --only-verified --fail .",
    "guard:no-shadcn": "node scripts/check-no-shadcn.mjs",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:e2e:update-snapshots": "PLAYWRIGHT_UPDATE_SNAPSHOTS=1 playwright test",
    "test:component": "playwright test -c playwright.component.config.ts",
    "test:report": "playwright show-report",
    "load:booking-matrix": "tsx scripts/booking-matrix-load.ts",
    "test:e2e:matrix": "playwright test tests/e2e/reservations/booking-validation-matrix.spec.ts --project=chromium",
    "load:booking-pack": "tsx scripts/booking-pack-fill.ts",
    "observability:baseline": "tsx scripts/observability/export-capacity-baselines.ts",
    "observability:export": "tsx scripts/observability/export-capacity-metrics.ts",
    "flags:audit": "tsx scripts/feature-flags/audit.ts",
    "email:check": "tsx scripts/email/check-resend-status.ts",
    "email:preview": "tsx -r tsconfig-paths/register scripts/preview-booking-email.ts",
    "jobs:hold-sweeper": "tsx -r tsconfig-paths/register scripts/jobs/run-hold-sweeper.ts",
    "jobs:allocations-pruner": "tsx -r tsconfig-paths/register scripts/jobs/run-allocations-pruner.ts",
    "queue:email-worker": "tsx -r tsconfig-paths/register scripts/queues/email-worker.ts",
    "assign:loop": "tsx -r tsconfig-paths/register scripts/ops-auto-assign-ultra-fast-loop.ts --slug white-horse-pub-waterbeach --date 2025-11-10",
    "booking:flow": "tsx -r tsconfig-paths/register scripts/run-booking-flow.ts",
    "db:reset": "echo 'ðŸ”„ Resetting remote database...' && source .env.local && psql \"$SUPABASE_DB_URL\" -v ON_ERROR_STOP=1 -f supabase/utilities/init-database.sql && psql \"$SUPABASE_DB_URL\" -v ON_ERROR_STOP=1 -f supabase/utilities/init-seeds.sql && echo 'âœ… Reset complete!'",
    "db:migrate": "echo 'ðŸ“Š Applying migrations to remote...' && source .env.local && psql \"$SUPABASE_DB_URL\" -v ON_ERROR_STOP=1 -f supabase/utilities/init-database.sql && echo 'âœ… Migrations applied!'",
    "db:seed-only": "echo 'ðŸŒ± Seeding remote database...' && source .env.local && psql \"$SUPABASE_DB_URL\" -v ON_ERROR_STOP=1 -f supabase/utilities/init-seeds.sql && echo 'âœ… Seeds applied!'",
    "db:run-allocation-test": "./scripts/run-allocation-stress-test.sh",
    "db:full-reset": "echo 'ðŸ”„ Full reset: applying migrations and seeds...' && pnpm run db:reset",
    "db:wipe": "echo 'âš ï¸  WARNING: Dropping public schema on REMOTE. Ensure you have a backup!' && source .env.local && psql \"$SUPABASE_DB_URL\" -v ON_ERROR_STOP=1 -f supabase/utilities/wipe-public-schema.sql && echo 'âœ… Schema wiped'",
    "db:verify": "./scripts/verify-database.sh",
    "db:test": "./scripts/test-connection.sh",
    "db:push": "echo 'ðŸ“¤ Pushing migrations to Supabase remote...' && supabase db push",
    "db:pull": "echo 'ðŸ“¥ Pulling schema from Supabase remote...' && supabase db pull",
    "db:status": "supabase migration list"
  },
