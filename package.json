  "private": true,
  "lint-staged": {
    "reserve/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "server/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=0"
