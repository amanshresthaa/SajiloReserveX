{
  "name": "ship-fast-code",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "reserve/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint reserve --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit -p tsconfig.strict.json",
    "validate:env": "tsx scripts/validate-env.ts",
    "prebuild": "npm run validate:env",
    "predev": "npm run validate:env",
    "reserve:dev": "vite --config reserve/vite.config.ts",
    "reserve:build": "vite build --config reserve/vite.config.ts",
    "test": "vitest run --config reserve/vitest.config.ts",
    "test:watch": "vitest --config reserve/vitest.config.ts",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006 -c reserve/.storybook",
    "storybook:build": "storybook build -c reserve/.storybook -o reserve/storybook-static",
    "analyze": "vite build --config reserve/vite.config.ts --mode analyze",
    "prepare": "husky",
    "secret:scan": "gitleaks detect --no-banner --redact && trufflehog filesystem --no-update --only-verified --fail .",
    "guard:no-shadcn": "node scripts/check-no-shadcn.mjs",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:e2e:update-snapshots": "PLAYWRIGHT_UPDATE_SNAPSHOTS=1 playwright test",
    "test:component": "playwright test -c playwright.component.config.ts",
    "test:report": "playwright show-report",
    "db:reset": "supabase db reset",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:studio": "open http://127.0.0.1:54323",
    "db:inbucket": "open http://127.0.0.1:54324",
    "db:seed": "supabase db reset",
    "db:status": "supabase status",
    "db:verify": "./scripts/verify-seed-final.sh",
    "db:test": "./scripts/test-connection.sh"
  },
