{
  "name": "ship-fast-code",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "reserve/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint reserve --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "reserve:dev": "vite --config reserve/vite.config.ts",
    "reserve:build": "vite build --config reserve/vite.config.ts",
    "test": "vitest run --config reserve/vitest.config.ts",
    "test:watch": "vitest --config reserve/vitest.config.ts",
    "test:e2e": "playwright test",
    "analyze": "vite build --config reserve/vite.config.ts --mode analyze",
    "prepare": "husky install",
    "secret:scan": "gitleaks detect --no-banner --redact && trufflehog filesystem --no-update --only-verified --fail ."
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@hookform/resolvers": "^3.9.1",
